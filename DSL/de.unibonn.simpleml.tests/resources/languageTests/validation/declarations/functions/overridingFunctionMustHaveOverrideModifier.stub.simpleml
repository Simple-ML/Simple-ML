package tests.overridingFunctionMustHaveOverrideModifier

open class MyOpenClass {
    open fun classFunction1()
    open fun classFunction2()
    static fun staticFunction()
}

class MyClass sub MyOpenClass {
    // no_semantic_error "An overriding function must have the override modifier."
    fun »ownFunction«()

    // semantic_error "An overriding function must have the override modifier."
    fun »classFunction1«()
    // no_semantic_error "An overriding function must have the override modifier."
    override fun »classFunction2«()
    // no_semantic_error "An overriding function must have the override modifier."
    fun »staticFunction«()
}
