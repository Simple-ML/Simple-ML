package simpleml.model.regression

import simpleml.model.*
import simpleml.dataset.Dataset

open class Lasso(regularizationStrength: Float or 0.5) sub Estimator {
    attr regularizationStrength: Float

    override fun fit(features: Dataset, target: Dataset) -> trainedModel: TrainedLasso
}

class TrainedLasso sub Model {
    attr slope: Int

    attr yIntercept: Int

    override fun predict(features: Dataset) -> results: Any
}
