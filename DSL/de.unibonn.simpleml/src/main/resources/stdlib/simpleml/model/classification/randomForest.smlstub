package simpleml.model.classification

import simpleml.model.*
import simpleml.dataset.Dataset

open class RandomForestClassifier() sub Estimator {
    attr nEstimator: Int?
    attr criterion: String?
    attr maxDepth: Int?
    attr randomState: Int?

    override fun fit(features: Dataset, target: Dataset) -> trainedModel: RandomForestModel
}

class RandomForestModel sub Model {
    override fun predict(features: Dataset) -> results: Any
}
