package simpleml.model.regression

import simpleml.model.*
import simpleml.dataset.Dataset

class DecisionTree(maxDepth: Int) sub Estimator {
    attr maxDepth: Int?

    fun fit(features: Dataset, target: Dataset) -> trainedModel: TrainedDecisionTree
}

class TrainedDecisionTree sub DecisionTree {
    fun predict(features: Dataset) -> results: Any
}
