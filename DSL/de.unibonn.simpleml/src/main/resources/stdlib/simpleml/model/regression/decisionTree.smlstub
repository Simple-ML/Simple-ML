package simpleml.model.regression

import simpleml.model.*
import simpleml.dataset.Dataset

class DecisionTreeRegressor(
    maxDepth: Int? = null
) sub Estimator {
    attr maxDepth: Int?

    fun fit(features: Dataset, target: Dataset) -> trainedModel: DecisionTreeRegressorModel
}

class DecisionTreeRegressorModel() sub Model {
    fun predict(features: Dataset) -> results: Any
}
