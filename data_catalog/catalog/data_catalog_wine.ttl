@base <https://simple-ml.de/resource/> .

@prefix void: <http://rdfs.org/ns/void#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .

# Simple-ML Wine Catalog

<WineDomain> rdf:type <Domain> .

<SimpleMLCatalogWine> rdf:type dcat:Catalog ;
    dcterms:title "Simple-ML Wine Data Catalog"@en ;
    foaf:homepage <https://simple-ml.de/> ;
    dcat:dataset <WhiteWineQualityBinary> ;
    dcat:dataset <RedWineQualityBinary> ;
    dcat:dataset <WhiteWineQuality> ;
    dcat:dataset <RedWineQuality> ;
    dcterms:subject "wine"@en ;
    dcterms:subject "Wein"@de ;
    <catalogDomain> <WineDomain> .
    